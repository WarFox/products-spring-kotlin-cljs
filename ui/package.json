{
    "name": "app",
    "scripts": {
        "ancient": "clojure -Sdeps '{:deps {com.github.liquidz/antq {:mvn/version \"RELEASE\"}}}' -m antq.core",
        "watch": "npx shadow-cljs watch app browser-test karma-test",
        "watch:css": "npx postcss src/css/tailwind.css -o resources/public/css/app.css --watch",
        "build:css": "npx postcss src/css/tailwind.css -o resources/public/css/app.css",
        "release": "npx shadow-cljs release app",
        "build-report": "npx shadow-cljs run shadow.cljs.build-report app target/build-report.html",
        "ci": "npx shadow-cljs compile karma-test && npx karma start --single-run --reporters junit,dots"
    },
    "dependencies": {
        "react": "17.0.2",
        "react-dom": "17.0.2"
    },
    "devDependencies": {
        "shadow-cljs": "2.26.2",
        "karma": "6.4.0",
        "karma-chrome-launcher": "3.1.1",
        "karma-cljs-test": "0.1.0",
        "karma-junit-reporter": "2.0.1",
        "autoprefixer": "^10.4.16",
        "postcss": "^8.4.31",
        "postcss-cli": "^10.1.0",
        "tailwindcss": "^3.3.5"
    }
}
