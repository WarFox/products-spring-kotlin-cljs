{
    "name": "app",
    "scripts": {
        "ancient": "clojure -Sdeps '{:deps {com.github.liquidz/antq {:mvn/version \"RELEASE\"}}}' -m antq.core",
        "watch": "npx shadow-cljs watch app browser-test karma-test",
        "watch:css": "npx @tailwindcss/cli -i src/css/tailwind.css -o resources/public/css/app.css --watch",
        "build:css": "npx @tailwindcss/cli -i src/css/tailwind.css -o resources/public/css/app.css",
        "release": "npx shadow-cljs release app",
        "build-report": "npx shadow-cljs run shadow.cljs.build-report app target/build-report.html",
        "ci": "npx shadow-cljs compile karma-test && npx karma start --single-run --reporters junit,dots"
    },
    "dependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@tailwindcss/cli": "^4.0.17",
        "karma": "6.4.0",
        "karma-chrome-launcher": "3.1.1",
        "karma-cljs-test": "0.1.0",
        "karma-junit-reporter": "2.0.1",
        "shadow-cljs": "2.28.22",
        "tailwindcss": "^4.0.17"
    }
}
